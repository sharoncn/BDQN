<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="header_white">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="header_black">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="reg_tv">
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/blue</item>
    </style>

    <style name="reg_et">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

   	<style name="reg_btn">
        <item name="android:background">@drawable/reg_btn_selector</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    
   	<style name="login_tv">
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/blue</item>
    </style>
    
    <style name="login_et">
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="about_tv">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="about_tv_item" parent="about_tv">
        <item name="android:layout_marginLeft">25dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>
    </style>
    <style name="about_tv_item_italic" parent="about_tv_item">
        <item name="android:textStyle">italic</item>
    </style>
    <style name="about_img">
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginRight">8dp</item>
    </style>
</resources>