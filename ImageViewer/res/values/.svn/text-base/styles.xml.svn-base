<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="dialog_title" parent="dialog_margin_buttom_left">
        <item name="android:textColor">#FF2E9AFE</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="dialog_margin_buttom">
        <item name="android:layout_marginBottom">10dp</item>
    </style>
    
   	<style name="dialog_margin_buttom_left" parent="dialog_margin_buttom">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
    </style>
        
    <style name="dialog_content" parent="dialog_margin_buttom_left">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">12sp</item>
        <item name="android:inputType">textMultiLine</item>
    </style>
    <style name="dialog_delete_button">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:gravity">center</item>
    </style>
</resources>