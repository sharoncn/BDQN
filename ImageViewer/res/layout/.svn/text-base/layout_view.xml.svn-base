<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <rjfsdo.sharoncn.android.BDQN.ImageViewer.Components.MyGallery
        android:id="@+id/view_gallery"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:spacing="10dp" />

    <rjfsdo.sharoncn.android.BDQN.ImageViewer.Components.ViewHeader
        android:id="@+id/view_header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:visibility="gone" />

    <rjfsdo.sharoncn.android.BDQN.ImageViewer.Components.DefaultMenu
        android:id="@+id/view_menu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:visibility="gone" />

</RelativeLayout>
