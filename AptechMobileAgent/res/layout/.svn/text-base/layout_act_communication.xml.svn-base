<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:rjf="http://schemas.android.com/apk/res/rjfsdo.sharoncn.android.BDQN.AptechMobileAgent"
    style="@style/layoutBothMatch"
    android:background="@drawable/bg_1"
    android:orientation="vertical" >

    <rjfsdo.sharoncn.android.BDQN.AptechMobileAgent.Components.DefaultHeader
        android:id="@+id/communication_header"
        style="@style/defaultHeaderStyle"
        android:background="@drawable/top_bg"
        rjf:headerTitle="@string/communication_mgr" />

    <Button
        android:id="@+id/communication_bakup"
        style="@style/communication_btn"
        android:background="@drawable/common_btn"
        android:text="@string/communication_bak" />

    <Button
        android:id="@+id/communication_restore"
        style="@style/communication_btn"
        android:background="@drawable/common_btn"
        android:text="@string/communication_restore" />

</LinearLayout>