package rjfsdo.sharoncn.android.CartoonReaderWeb.Model;

import java.io.Serializable;

public class BookInfo implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	private int id;
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	private String bookId;//id
	private String bookName;//书名
	private String bookContent;//简介
	private String coverPath;//封面路径(SDCard中的路径)
	private String zipPath;//压缩包路径(SDCard中的路径)
	private String writer;//作者
	private String countryName;//国家
	private String classId;//所属类别编号
	private String className;//所属类编名称
	private String bookSize;//漫画大小
	private String createDate;//创建日期
	private String hitNum;//点击数量
	private String grade;//评价星级
	private String gradeNums;//评价数量
	private String recommend;//推荐标记
	private String page;//阅读页数
	private String pageTotal;//总页数
	private String lastReadDate;//最后阅读时间
	private String lastReadedPage;//最后阅读页数
	
	public BookInfo(){}
	
	@Override
	public String toString() {
		return "bookId:" + bookId + "/" + "bookName:" + bookName+ "/" + "bookContent:" + bookContent+ "/" + "coverPath:" 
	+ coverPath + "/" + "zipPath:" + zipPath + "/" + "writer:" + writer + "/" + "countryName:" + countryName + "/" 
				+ "classId:" + classId+ "/" + "className:" +className+ "/" + "bookSize:" + bookSize+ "/" + "/" 
	+ "createDate:" + createDate + "/" + "hitNum:" + hitNum + "/" + "grade:" + grade+ "/" + "gradeNums:" + gradeNums+ "/" 
				+ "recommend:" + recommend + "/" + "page:" + lastReadedPage + "/" + "lastReadDate:" + lastReadDate + "pageTotal:" + pageTotal;
	}


	public BookInfo(int id,String bookId, String bookName, String bookContent,
			String bookCoverPath, String bookZipPath, String writer,
			String countryName, String classId, String className,
			String bookSize, String createDate, String hitNum,
			String bookGrade, String bookGradeNums, String recommend,
			String page, String pageTotal,String lastReadDate,String lastReadedPage) {
		super();
		this.id = id;
		this.bookId = bookId;
		this.bookName = bookName;
		this.bookContent = bookContent;
		this.coverPath = bookCoverPath;
		this.zipPath = bookZipPath;
		this.writer = writer;
		this.countryName = countryName;
		this.classId = classId;
		this.className = className;
		this.bookSize = bookSize;
		this.createDate = createDate;
		this.hitNum = hitNum;
		this.grade = bookGrade;
		this.gradeNums = bookGradeNums;
		this.recommend = recommend;
		this.page = page;
		this.pageTotal = pageTotal;
		this.lastReadDate = lastReadDate;
		this.lastReadedPage = lastReadedPage;
	}

	public String getBookId() {
		return bookId;
	}

	public void setBookId(String bookId) {
		this.bookId = bookId;
	}

	public String getBookName() {
		return bookName;
	}

	public void setBookName(String bookName) {
		this.bookName = bookName;
	}

	public String getBookContent() {
		return bookContent;
	}

	public void setBookContent(String bookContent) {
		this.bookContent = bookContent;
	}

	public String getCoverPath() {
		return coverPath;
	}

	public void setCoverPath(String coverPath) {
		this.coverPath = coverPath;
	}

	public String getZipPath() {
		return zipPath;
	}

	public void setZipPath(String zipPath) {
		this.zipPath = zipPath;
	}

	public String getWriter() {
		return writer;
	}

	public void setWriter(String writer) {
		this.writer = writer;
	}

	public String getCountryName() {
		return countryName;
	}

	public void setCountryName(String countryName) {
		this.countryName = countryName;
	}

	public String getClassId() {
		return classId;
	}

	public void setClassId(String classId) {
		this.classId = classId;
	}

	public String getClassName() {
		return className;
	}

	public void setClassName(String className) {
		this.className = className;
	}

	public String getBookSize() {
		return bookSize;
	}

	public void setBookSize(String bookSize) {
		this.bookSize = bookSize;
	}

	public String getCreateDate() {
		return createDate;
	}

	public void setCreateDate(String createDate) {
		this.createDate = createDate;
	}

	public String getHitNum() {
		return hitNum;
	}

	public void setHitNum(String hitNum) {
		this.hitNum = hitNum;
	}

	public String getGrade() {
		return grade;
	}

	public void setGrade(String grade) {
		this.grade = grade;
	}

	public String getGradeNums() {
		return gradeNums;
	}

	public void setGradeNums(String gradeNums) {
		this.gradeNums = gradeNums;
	}

	public String getRecommend() {
		return recommend;
	}

	public void setRecommend(String recommend) {
		this.recommend = recommend;
	}

	public String getPage() {
		return page;
	}

	public void setPage(String page) {
		this.page = page;
	}

	public String getPageTotal() {
		return pageTotal;
	}

	public void setPageTotal(String pageTotal) {
		this.pageTotal = pageTotal;
	}

	public String getLastReadDate() {
		return lastReadDate;
	}

	public void setLastReadDate(String lastReadDate) {
		this.lastReadDate = lastReadDate;
	}

	public String getLastReadedPage() {
		return lastReadedPage;
	}

	public void setLastReadedPage(String lastReadedPage) {
		this.lastReadedPage = lastReadedPage;
	}
}
